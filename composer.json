{
    "name": "zhulanov111/serializer",
    "description": "Php library for serializing data of various objects into formats json and xml",
    "type": "library",
    "authors": [
        {
            "name": "Konstantin Zhulanov",
            "email": "zhulanov111@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "symfony/yaml": "^4.3@dev",
        "php": ">=7.2",
        "ext-json": "*"
    },
    "autoload": {
        "psr-4": {
            "Zhulanov111\\Serializer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Zhulanov111\\Tests\\": "tests/"
        }
    }
}
